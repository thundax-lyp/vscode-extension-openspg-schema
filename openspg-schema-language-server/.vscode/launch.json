{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Launch Client",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}", "${workspaceRoot}/client/testFixture/"],
			"autoAttachChildProcesses": true,
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/client/dist/**/*.js"],
			"preLaunchTask": "npm: watch"
		}
	]
}
