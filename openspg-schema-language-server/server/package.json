{
    "name": "openspg-schema-language-server",
    "version": "0.2.0-dev-0",
    "description": "Language Server Protocol implementation for OpenSPG Schema",
    "keywords": [
        "openspg",
        "language-server",
        "lsp",
        "vscode",
        "extension"
    ],
    "license": "Apache-2.0",
    "author": "Thundax <thundax@qq.com>",
    "homepage": "https://github.com/thundax-lyp/vscode-extension-openspg-schema",
    "categories": [
        "Programming Languages"
    ],
    "publisher": "thundax-lyp",
    "module": "./dist/index.js",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "engines": {
        "vscode": "^1.100.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "scripts": {
        "vscode:prepublish": "npm run build",
        "build": "unbuild --clean"
    },
    "dependencies": {
        "openspg-concept-rule-antlr4": "file://./openspg-concept-rule-antlr4-0.2.0-dev-0.tgz",
        "openspg-schema-antlr4": "file://./openspg-schema-antlr4-0.2.0-dev-0.tgz",
        "prettier": "^3.6.2",
        "type-fest": "^5.2.0",
        "vscode-languageclient": "^9.0.1",
        "vscode-languageserver": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.12",
        "vscode-uri": "^3.1.0"
    },
    "devDependencies": {
        "@types/vscode": "^1.100.0",
        "unbuild": "^3.6.1"
    }
}
