{
    "name": "openspg-concept-rule-antlr4",
    "description": "OpenSPG Concept Rule Mark Language Lexer and Parser by official ANTLR4 grammar",
    "version": "0.1.4",
    "keywords": [
        "openspg",
        "schema",
        "ast",
        "parser",
        "lexer",
        "antlr4"
    ],
    "license": "Apache-2.0",
    "type": "module",
    "module": "./dist/index.js",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./ast": {
            "import": "./dist/ast/index.js",
            "require": "./dist/ast/index.cjs",
            "types": "./dist/ast/index.d.ts"
        },
        "./antlr4": {
            "import": "./dist/antlr4/index.js",
            "require": "./dist/antlr4/index.cjs",
            "types": "./dist/antlr4/index.d.ts"
        },
        "./parser": {
            "import": "./dist/parser/index.js",
            "require": "./dist/parser/index.cjs",
            "types": "./dist/parser/index.d.ts"
        },
        "./traverse": {
            "import": "./dist/traverse/index.js",
            "require": "./dist/traverse/index.cjs",
            "types": "./dist/traverse/index.d.ts"
        },
        "./prettier": {
            "import": "./dist/prettier/index.js",
            "require": "./dist/prettier/index.cjs",
            "types": "./dist/prettier/index.d.ts"
        },
        "./generator": {
            "import": "./dist/generator/index.js",
            "require": "./dist/generator/index.cjs",
            "types": "./dist/generator/index.d.ts"
        }
    },
    "files": [
        "dist",
        "grammar",
        "README.md"
    ],
    "sideEffects": false,
    "author": "thundax <thundax@qq.com>",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/thundax-lyp/openspg-concept-rule-antlr4.git"
    },
    "lint-staged": {
        "*.ts": "eslint --quiet --fix",
        "*.{ts}": "prettier --loglevel warn --write"
    },
    "peerDependencies": {
        "prettier": "^3.0.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "scripts": {
        "grammar": "antlr-ng -Dlanguage=TypeScript -o src/antlr4/ -v -l --long-messages=true grammar/ConceptRuleLexer.g4 grammar/ConceptRuleParser.g4",
        "ast": "node scripts/init-ast-index.ts ./src/ast",
        "dev": "unbuild --stub",
        "build": "unbuild --clean",
        "docs": "typedoc --tsconfig ./tsconfig.json --skipErrorChecking --plugin typedoc-github-theme src/typedoc.ts",
        "prepublishOnly": "npm run build",
        "changeset": "changeset",
        "release": "changeset version && changeset publish",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "test:ui": "vitest --ui",
        "test:ci": "npm run test:coverage",
        "lint": "pnpm run \"/^lint:.+/\"",
        "lint:eslint": "eslint --quiet --fix --ext .ts .",
        "lint:prettier": "prettier --write \"{src,test}/**/*.{ts,tsx,js,jsx,json,css,less,md}\" --log-level warn",
        "husky:prepare": "husky install",
        "husky:pre-commit": "lint-staged"
    },
    "dependencies": {
        "antlr4ng": "^3.0.16",
        "lodash-es": "^4.17.23",
        "type-fest": "^5.4.3"
    },
    "devDependencies": {
        "@changesets/changelog-git": "^0.2.1",
        "@changesets/cli": "^2.29.8",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.19.7",
        "@typescript-eslint/parser": "^8.22.0",
        "@vitest/coverage-v8": "^2.1.9",
        "@vitest/ui": "^2.1.9",
        "antlr-ng": "^1.0.10",
        "changeset": "^0.2.6",
        "eslint": "^9.39.2",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",
        "typedoc": "^0.28.16",
        "typedoc-github-theme": "^0.3.1",
        "typescript": "~5.9.0",
        "unbuild": "^3.6.1",
        "vitest": "^2.1.9"
    }
}
